 number = {02},
 pages = {116-128},
 title = {Ekosistem Pesantren dalam Mewujudkan Manajemen Halal Supply Chain Menuju Madani Society 5.0},
 url = {https://app.dimensions.ai/details/publication/pub.1142777217},
