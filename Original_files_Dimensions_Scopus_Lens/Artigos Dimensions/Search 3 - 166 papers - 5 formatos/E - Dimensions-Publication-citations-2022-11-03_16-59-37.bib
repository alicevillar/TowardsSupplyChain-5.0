 title = {INDUSTRY 4.0 - DEVELOPMENT AND CONSEQUENCES FOR SUSTAINABLE DEVELOPMENT OF BULGARIA},
 url = {https://app.dimensions.ai/details/publication/pub.1138961021},
 volume = {3},
 year = {2021}
